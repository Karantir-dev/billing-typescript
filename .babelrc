{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@alias": "./src/your-folder",
          "@images": "./src/images",
          "@redux": "./src/Redux",
          "@components": "./src/Components",
          "@pages": "./src/Pages",
          "@utils": "./src/utils",
          "@src": "./src",
          "@config": "./src/config",
          "@scss": "./src/scss"
        }
      }
    ]
  ]
}
